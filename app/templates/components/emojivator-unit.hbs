<div class="emojivator-unit-target">
  <p>Your target output sequence is: {{problem.target}}</p>
</div>
<div class="emojivator-unit-input">
  {{emoji-picker emojis=problem.validInputs register-as=emojiPicker}}
  <button {{action "transformInput"}}>Go!</button>
</div>
<div class="emojivator-unit-output">
  <p class="emojivator-unit-output-text">{{output}}</p>
</div>
{{emojivator-history history=history}}
